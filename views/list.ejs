<%-include('header')-%>

  <div class="box" id="heading">
    <h1><%=kindOfDay%></h1>
  </div>

  <div class="box">
    <%items.forEach(item=>{%>
    <div class="item">
      <input type="checkbox">
      <p><%=item%></p>
    </div>
    <%})%>
    <form class="item" action="/" method="post">
      <input type="text" name="list_item" placeholder="New list item" autocomplete="off">
      <button type="submit" name="buttonName" value=<%=kindOfDay%>>+</button>
    </form>
  </div>
<%-include('footer')-%>
